datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}



model Twin {
  id           String   @id @default(uuid())
  specURL      String
  capabilities String?
  eventMeshURL String
  registeredAt DateTime @default(now())
  createdBy    String?

  @@unique([id, createdBy])
}
